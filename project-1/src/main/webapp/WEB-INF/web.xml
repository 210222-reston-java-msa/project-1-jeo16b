<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>project-1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FrontController</display-name>
    <servlet-name>FrontController</servlet-name>
    <servlet-class>com.revature.web.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontController</servlet-name>
    <url-pattern>/FrontController</url-pattern>
    <url-pattern>/login</url-pattern>
    <url-pattern>/logout</url-pattern>
    <url-pattern>/users</url-pattern>
    <url-pattern>/reimbursements</url-pattern>
    <url-pattern>/error</url-pattern>
    <url-pattern>/reimbursementsByUser</url-pattern>
    <url-pattern>/employee</url-pattern>
    <url-pattern>/manager</url-pattern>
    <url-pattern>/newReimbursement</url-pattern>
    <url-pattern>/reimByUser</url-pattern>
    <url-pattern>/pendingReimbursements</url-pattern>
    <url-pattern>/resolvedReimbursements</url-pattern>
    <url-pattern>/update</url-pattern>
    <url-pattern>/myPendingReimbursements</url-pattern>
    <url-pattern>/myPastReimbursements</url-pattern>
  </servlet-mapping>
  
   <error-page>
  	<error-code>404</error-code>
  	<location>/error</location>
  </error-page>
  
</web-app>